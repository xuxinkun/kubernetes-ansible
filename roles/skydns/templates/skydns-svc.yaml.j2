# From https://github.com/GoogleCloudPlatform/kubernetes/tree/master/cluster/addons/dns

kind: Service
apiVersion: v1beta1
id: kube-dns
namespace: default
protocol: UDP
port: 53
portalIP: {{ dns_server }}
containerPort: 53
labels:
  k8s-app: kube-dns
  name: kube-dns
  kubernetes.io/cluster-service: "true"
selector:
  k8s-app: kube-dns
